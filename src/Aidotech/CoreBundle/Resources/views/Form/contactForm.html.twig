<div class="col-md-8 col-md-offset-2">
  {% form_theme contactForm 'bootstrap_3_layout.html.twig' %}
  {{ form_start(contactForm) }}
  {{ form_errors(contactForm) }}
  {{ form_errors(contactForm.nom) }}
  {{ form_row(contactForm.nom) }}
  {{ form_errors(contactForm.prenom) }}
  {{ form_row(contactForm.prenom) }}
  {{ form_errors(contactForm.email) }}
  {{ form_row(contactForm.email) }}
  {{ form_errors(contactForm.objet) }}
  {{ form_row(contactForm.objet) }}
  {{ form_errors(contactForm.message) }}
  {{ form_row(contactForm.message) }}
  <button type="submit" id="form_envoyer" name="form[envoyer]" class="btn-primary btn">Envoyer votre message</button>
  {{ form_end(contactForm) }}
</div>